\NewDocumentCommand{\NewCommandIfNotDefined}{ mom }{%
  \ifx#1\undefinied%
    \IfNoValueTF{#2}{%
      \newcommand{#1}{#3}%
    }{%
      \newcommand{#1}[#2]{#3}%
    }%
  \else%
  \fi% 
}

\NewDocumentCommand{\NewEnvironmentIfNotDefined}{ moomm }{%
  \ifcsname#1\endcsname%
  \else%
    \IfNoValueTF{#2}{%
      \newenvironment{#1}{#4}{#5}%
    }{%
      \IfNoValueTF{#3}{%
        \newenvironment{#1}[#2]{#4}{#5}%
      }{%
        \expandafter\newenvironment{#1}[#2][#3]{#4}{#5}%
      }
    }%
  \fi%
}

\NewDocumentCommand{\NewTheoremIfNotDefined}{ mmo }{%
  \ifcsname#1\endcsname%
  \else%
    \IfNoValueTF{#3}{%
      \newtheorem{#1}{#2}%
    }{%
      \newtheorem{#1}{#2}[#3]%
    }%
  \fi%
}

\NewDocumentCommand{\DeclareMathOperatorIfNotDeclared}{ mmo }{
  \ifx#1\undefinied%
    \IfNoValueTF{#3}{%
      \DeclareMathOperator{#1}{#2}%
    }{%
      \DeclareMathOperator{#1}{#2}[#3]%
    }%
  \else%
  \fi% 
}

